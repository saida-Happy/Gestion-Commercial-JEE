<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
 <managed-bean>
  <managed-bean-name>Produit</managed-bean-name>
  <managed-bean-class>beans.Produit</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>users</managed-bean-name>
  <managed-bean-class>beans.Users</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>Commandes</managed-bean-name>
  <managed-bean-class>beans.Commandes</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>Clients</managed-bean-name>
  <managed-bean-class>beans.Clients</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>Client</managed-bean-name>
  <managed-bean-class>dao.Client</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <from-view-id>/Affiche.jsp</from-view-id>
  <navigation-case>
   <from-outcome>envoie</from-outcome>
   <to-view-id>/commander.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/Login.jsp</from-view-id>
  <navigation-case>
   <from-outcome>failure</from-outcome>
   <to-view-id>/Login.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>login</from-outcome>
   <to-view-id>/Affiche.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/commander.jsp</from-view-id>
  <navigation-case>
   <from-outcome>saveCmdSuccess</from-outcome>
   <to-view-id>/Affiche.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>saveCmdFulaire</from-outcome>
   <to-view-id>/commander.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>annulerCmd</from-outcome>
   <to-view-id>/Affiche.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/ModifierPwd.jsp</from-view-id>
  <navigation-case>
   <from-outcome>logout</from-outcome>
   <to-view-id>/Login.jsp</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>pageAccueil</from-outcome>
   <to-view-id>/Affiche.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/commande.jsp</from-view-id>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/clientsList.jsp</from-view-id>
  <navigation-case>
   <from-outcome>envoieC</from-outcome>
   <to-view-id>/addClient.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/addClient.jsp</from-view-id>
  <navigation-case>
   <from-outcome>ajoutC</from-outcome>
   <to-view-id>/clientsList.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
</faces-config>
